#! /usr/bin/env zsh

function main TRAPEXIT {
  # cd $HOME/.data-upper/pm
  # mux start --detached --name=organizer organizer

  cd $HOME/.data-upper/artifacts
  mux start --detached --name=notes notes

  cd $HOME/.dotfiles
  mux start --detached --name=dotfiles generic

  cd $HOME/.lingo
  mux start --detached --name=lingo lingo

  cd $HOME/.workspace/summaries
  mux start --detached --name=summaries generic

  cd $HOME/.workspace/dark_cam
  mux start --detached --name=dark_cam cpp

  cd $HOME/.workspace/tem18lngdm
  mux start --detached --name=tem18lngdm cpp

  cd $HOME/.workspace/sample_media_player
  mux start --detached --name=sample_media_player cpp

  cd $HOME/.workspace/asudpp
  mux start --detached --name=asudpp generic

  cd $HOME/.desktop
  mux start --detached --name=console console

  tmux attach-session -t console
}

